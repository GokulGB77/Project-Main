        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                    <p>View and Edit Existing Orders</p>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option hidden>Status</option>
                                <option>All</option>
                                <option>Pending</option>
                                <option>Delivered</option>
                                <option>Cancelled</option>
                                <option>Returned</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th class="text-center">Sl No</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Order ID</th>
                                    <th scope="col">Total</th>
                                    <th scope="col" class="text-center">Status</th>
                                    <th scope="col">Order Date</th>
                                    <th scope="col" style="padding-left: 20px;"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% allOrders.forEach((order,index)=>{ %>
                                    <tr>
                                        <td class="text-center">
                                            <%= index+1 %>
                                        </td>
                                        <td><b>
                                                <%= order.user ? order.user.name : 'Unknown' %>
                                            </b></td>
                                        <td>#<%= order.orderId %>
                                        </td>
                                        <td>
                                            &#8377; <%= (order.orderTotal).toLocaleString() %>
                                        </td>
                                        <% if (order.orderStatus==="pending" ) { %>
                                            <td class="text-center"><span
                                                    class="badge rounded-pill  alert-warning ">Pending</span>
                                            </td>
                                            <% } else if (order.orderStatus==="delivered" ) { %>
                                                <td class="text-center"><span
                                                        class="badge rounded-pill  alert-success">Delivered</span>
                                                </td>
                                                <% } else if(order.orderStatus==="cancelled" ) { %>
                                                    <td class="text-center"><span
                                                            class="badge rounded-pill  alert-danger">Cancelled</span>
                                                    </td>
                                                    <% } else if(order.orderStatus==="returned" ) {%>
                                                        <td class="text-center"><span
                                                                class="badge rounded-pill  alert-info">Returned</span>
                                                        </td>
                                                        <% } %>
                                                            <td>
                                                                <%= order.orderDate %>
                                                            </td>
                                                            <td>
                                                                <a href="/admin/orders/order-details?orderId=<%=order.orderId %>"
                                                                    class="btn btn-md rounded font-sm">Detail</a>
                                                                <!-- <div class="dropdown">
                                                                    <a href="#" data-bs-toggle="dropdown"
                                                                        class="btn btn-light rounded btn-sm font-sm"> <i
                                                                            class="material-icons md-more_horiz"></i>
                                                                    </a>
                                                                    <div class="dropdown-menu">
                                                                        <a class="dropdown-item" href="/admin/orders/order-details?orderId=<%=order.orderId %>">View
                                                                            detail</a>
                                                                        <a class="dropdown-item" href="#">Edit info</a>
                                                                        <a class="dropdown-item text-danger"
                                                                            href="#">Delete</a>
                                                                    </div>
                                                                </div>  -->
                                                                <!-- dropdown //end -->
                                                            </td>
                                    </tr>
                                    <% }) %>
                                        <style>
                                            .text-center {
                                                text-align: center;
                                            }
                                        </style>
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
